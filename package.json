{
	"name": "ccask",
	"version": "1.0.0",
	"description": "AI Question",
	"main": "dist/index.js",
	"bin": "dist/index.js",
	"module": "src/index.tsx",
	"type": "module",
	"scripts": {
		"dev": "bun run src/index.tsx",
		"build": "bun build src/index.tsx --outdir dist --target=node && sed -i '' '1s|#!/usr/bin/env bun|#!/usr/bin/env node|' dist/index.js",
		"prepublishOnly": "bun run build",
		"test": "bun test",
		"typecheck": "tsgo --noEmit"
	},
	"dependencies": {
		"@effect/platform": "^0.85.2",
		"@effect/platform-bun": "^0.70.4",
		"@types/react": "^19.1.8",
		"effect": "^3.8.0",
		"ink": "^6.0.1",
		"react": "^19.1.0"
	},
	"devDependencies": {
		"@types/bun": "latest"
	},
	"peerDependencies": {
		"@typescript/native-preview": "^7.0.0-dev.20250619.1"
	}
}
